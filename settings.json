{
  "editor.renderLineHighlight": "gutter",

  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 2,

  "editor.parameterHints.enabled": false,

  "workbench.colorTheme": "Omni",
    
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",

  "workbench.editor.labelFormat": "short",
  "workbench.editorAssociations": [
    {
        "viewType": "jupyter.notebook.ipynb",
        "filenamePattern": "*.ipynb"
    }
  ],

  "explorer.compactFolders": false,
  
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.cursorStyle": "line",

  "emmet.syntaxProfiles": { "java": "jsx" },
  "emmet.includeLanguages": { "java": "javareact" },

  "java.updateImportsOnFileMove.enabled": "never",
  "type.updateImportsOnFileMove.enabled": "never",

  "java.suggest.autoImports": true,
  "type.suggest.autoImports": true,

  "breadcrumbs.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,

  "C_Cpp.updateChannel": "Insiders",
  "sync.gist": "dd411e57d723a8d3f5c3d57b9048a5a5",
  "cSpell.userWords": [
      "keycloak",
      "mariadb",
      "mysqladmin"
  ],
  "cSpell.enabled": false,
  "git.autofetch": true,
  "window.zoomLevel": 1,
  
  "git.suggestSmartCommit": false,
}
